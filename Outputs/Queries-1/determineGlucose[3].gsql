CREATE QUERY determineGlucose(UINT hraID) {
/**
* Build out Smoker Condition
**/
  INT id;
  INT glucose;
  BOOL dibetic = FALSE;
  Start = {HRA.*};

  HRS = SELECT h FROM Start:h WHERE h.id == hraID ACCUM id=h.id, glucose=h.glucose, 
  IF h.glucose >= 126 THEN dibetic=TRUE END
  POST-ACCUM  
             INSERT INTO Glucose VALUES(id,glucose,dibetic),
             INSERT INTO condition VALUES(id Person, id Glucose,"Glucose");

  PRINT id,glucose,dibetic;
}